generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id         Int      @id @default(autoincrement())
  unique_id  String   @default(uuid())
  first_name String
  last_name  String
  email      String   @unique
  password   String
  created_at DateTime @default(now())
}
